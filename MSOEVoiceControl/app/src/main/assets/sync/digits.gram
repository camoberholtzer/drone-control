#JSGF V1.0;

grammar digits;

<digit> = forward    |
          move forward |
          backward  |
          move backward |
          up  |
          fly up |
          down  |
          fly down|
          right   |
          turn right |
          finish |
          land |
          left  |
          turn left |
          reverse |
          turn around;

public <digits> = <digit>+;
